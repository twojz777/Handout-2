def assign_grade(score):
    if score < 0 or score > 100:
        print("0 - 100 only")
    elif score >= 90:
        print("Grade: A")
    elif score >= 80:
        print("Grade: B")
    elif score >= 70:
        print("Grade: D")
    elif score >= 60:
        print("Grade: D")
    else:
        print("Grade: F")

score = input("Enter score here: ")
score = int(score)
assign_grade(score)
